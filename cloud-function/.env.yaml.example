# Cloud Function Environment Variables
# Copy this file to .env.yaml and fill in your values

# Your Gemini API Key from https://makersuite.google.com/app/apikey
GEMINI_API_KEY: "YOUR_GEMINI_API_KEY_HERE"

# Chrome Extension ID (get from Chrome Web Store after publishing)
EXTENSION_ID: "YOUR_CHROME_EXTENSION_ID_HERE"

# Optional: Environment setting
NODE_ENV: "production"